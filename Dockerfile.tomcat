FROM ubuntu:18.04
RUN apt update -y
RUN apt install git default-jdk wget -y
WORKDIR /opt/tomcat
RUN wget https://archive.apache.org/dist/tomcat/tomcat-10/v10.1.19/bin/apache-tomcat-10.1.19.tar.gz
RUN tar zxvf apache-tomcat-*.tar.gz
EXPOSE 8080
CMD ["/opt/tomcat/apache-tomcat-10.1.19/bin/catalina.sh", "run"]
